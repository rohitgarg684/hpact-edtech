# LangChain Web Content Processing Pipeline Configuration

# Required Environment Variables
OPENAI_API_KEY=your_openai_api_key_here

# Qdrant Vector Database Configuration
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_COLLECTION_NAME=langchain_knowledge_graph

# OpenAI Model Configuration (optional, defaults provided)
OPENAI_MODEL=gpt-3.5-turbo
OPENAI_TEMPERATURE=0.1
OPENAI_EMBEDDING_MODEL=text-embedding-3-large
EMBEDDING_SIZE=3072

# Web Crawler Configuration (optional)
WEB_CRAWLER_TIMEOUT=10
WEB_CRAWLER_MAX_CONTENT_LENGTH=50000